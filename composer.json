{
    "name": "eventsauce/doctrine-message-repository",
    "type": "library",
    "description": "Doctrine Message Repository for EventSauce",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank de Jonge",
            "email": "info@frenky.net"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "doctrine/dbal": "^2.5",
        "eventsauce/eventsauce": "^0.7.0 || ^0.8.0",
        "ramsey/uuid": "^3.6 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "EventSauce\\DoctrineMessageRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventSauce\\DoctrineMessageRepository\\Tests\\": "tests"
        }
    }
}
